{% extends "md_view_edit.ttml" %}


{% block content %}
<div class="nodeEdit" id="{{node.name}}">

  
<h1 class="nodeName">{{node.name}}</h1>


<div class="defaultView" id="view{{node.name}}">
{{node.content}}
</div>



</div>

<script>
  (function () {
  	var htmlContent = convertMarkdown(d3.select("#view{{node.name}}").html());
  	d3.select("view{{node.name}}").html(htmlContent);
	//d3.select("#view{{node.name}}").html(htmlContent);
})();
</script>
{% endblock content %}
